.i-Partners
  padding 6.1rem 0 6.7rem
  background-color #FBFBFB
  &__heading
    font-size: 2.4rem
    font-weight: 300
    line-height: 3.8rem
    text-align center
    color black
    margin-bottom 6rem
  &__container
    display flex
    justify-content space-between
    align-items center

@media (max-width 1000px)
  .i-Partners
    &__container
      flex-wrap wrap
    &__wrap
      width 50%
      text-align center
      margin-bottom 2rem
      & img
        width: 100%;
        max-width: 16rem;
        object-fit: contain;
